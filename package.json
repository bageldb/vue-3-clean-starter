{
  "name": "bagellink-starter",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
		"es:lint": "eslint . --cache --debug -c eslint.config.js --cache-location ./node_modules/.cache/.eslintcache",
		"es:lint:fix": "eslint . --cache --fix --debug -c eslint.config.js --cache-location ./node_modules/.cache/.eslintcache",
		"osc:lint": "oxlint .",
		"osc:lint:fix": "oxlint . --fix",
		"lint": "bun run osc:lint && bun run es:lint",
		"lint:fix": "bun run osc:lint:fix && bun run es:lint:fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "pinia": "^2.1.7",
		"vue": "^3.4.27",
		"vue-router": "^4.3.2"
  },
  "devDependencies": {
		"@rushstack/eslint-patch": "^1.10.3",
		"@tsconfig/node18": "^18.2.4",
		"@types/jsdom": "^21.1.7",
		"@types/node": "^20.14.0",
		"@vitejs/plugin-vue": "^5.0.5",
		"@vitejs/plugin-vue-jsx": "^4.0.0",
		"@vue/eslint-config-prettier": "^9.0.0",
		"@vue/eslint-config-typescript": "^13.0.0",
		"@vue/test-utils": "^2.4.6",
		"@vue/tsconfig": "^0.5.1",
		"jsdom": "^24.1.0",
		"npm-run-all2": "^6.2.0",
		"prettier": "^3.3.0",
		"typescript": "~5.4.5",
		"vite": "^5.2.12",
		"vitest": "^1.6.0",
		"vue-tsc": "^2.0.19",
		"@typescript-eslint/eslint-plugin": "^7.11.0",
		"@stylistic/eslint-plugin-js": "^2.1.0",
		"eslint": "^9.4.0",
    "eslint-config-airbnb-base": "^15.0.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-vue": "^9.26.0",
		"eslint-plugin-vuejs-accessibility": "^2.3.0",
		"@vue-macros/reactivity-transform": "^0.4.6",
		"@antfu/eslint-config": "^2.20.0",
		"@stylistic/eslint-plugin": "^2.1.0",
		"@types/eslint": "^8.56.10",
		"@types/eslint__eslintrc": "^2.1.1",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-format": "^0.1.1",
		"eslint-plugin-html": "^8.1.1",
		"eslint-plugin-import-x": "^0.5.1",
		"eslint-plugin-oxlint": "^0.4.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-vitest": "^0.5.4",
		"eslint-plugin-vue-scoped-css": "^2.8.0",
		"oxlint": "^0.4.2",
		"typescript-eslint": "^7.11.0",
		"vue-eslint-parser": "^9.4.3"
  },
  "volta": {
    "node": "20.10.0"
  }
}
