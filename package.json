{
  "name": "bagellink-starter",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
		"es:lint": "eslint . --cache --debug -c eslint.config.js --cache-location ./node_modules/.cache/.eslintcache",
		"es:lint:fix": "eslint . --cache --fix --debug -c eslint.config.js --cache-location ./node_modules/.cache/.eslintcache",
		"osc:lint": "oxlint .",
		"osc:lint:fix": "oxlint . --fix",
		"lint": "bun run osc:lint && bun run es:lint",
		"lint:fix": "bun run osc:lint:fix && bun run es:lint:fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "pinia": "^2.2.6",
		"vue": "^3.5.13",
		"vue-router": "^4.4.5"
  },
  "devDependencies": {
		"@rushstack/eslint-patch": "^1.10.4",
		"@tsconfig/node18": "^18.2.4",
		"@types/jsdom": "^21.1.7",
		"@types/node": "^22.9.0",
		"@vitejs/plugin-vue": "^5.2.0",
		"@vitejs/plugin-vue-jsx": "^4.1.0",
		"@vue/eslint-config-prettier": "^10.1.0",
		"@vue/eslint-config-typescript": "^14.1.3",
		"@vue/test-utils": "^2.4.6",
		"@vue/tsconfig": "^0.6.0",
		"jsdom": "^25.0.1",
		"npm-run-all2": "^7.0.1",
		"prettier": "^3.3.3",
		"typescript": "~5.6.3",
		"vite": "^5.4.11",
		"vite-plugin-vue-devtools": "^7.6.4",
		"vitest": "^2.1.5",
		"vue-tsc": "^2.1.10",
		"@typescript-eslint/eslint-plugin": "^8.14.0",
		"@stylistic/eslint-plugin-js": "^2.10.1",
		"eslint": "^9.15.0",
    "eslint-config-airbnb-base": "^15.0.0",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-vue": "^9.31.0",
		"eslint-plugin-vuejs-accessibility": "^2.4.1",
		"@vue-macros/reactivity-transform": "^1.1.3",
		"@antfu/eslint-config": "^3.9.1",
		"@stylistic/eslint-plugin": "^2.10.1",
		"@types/eslint": "^9.6.1",
		"@types/eslint__eslintrc": "^2.1.2",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-format": "^0.1.2",
		"eslint-plugin-html": "^8.1.2",
		"eslint-plugin-import-x": "^4.4.2",
		"eslint-plugin-oxlint": "^0.11.1",
		"eslint-plugin-prettier": "^5.2.1",
		"eslint-plugin-vitest": "^0.5.4",
		"eslint-plugin-vue-scoped-css": "^2.8.1",
		"oxlint": "^0.11.1",
		"typescript-eslint": "^8.14.0",
		"vue-eslint-parser": "^9.4.3"
  },
  "volta": {
    "node": "20.10.0"
  }
}
