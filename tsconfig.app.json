{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts",
  "src/**/*",
  "src/**/*.vue",
  "src/**/*.ts",
  "src/**/*.d.ts",
  "src/**/*.tsx",
  "src/**/*.vue",
  "./**/*.ts",
  "./**/*.js",
  "./.eslintrc.cjs",
  "vitest.config.ts",
	"prettier.config.cjs"
],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
		"types": ["@vue-macros/reactivity-transform/macros-global"]
  }
}
